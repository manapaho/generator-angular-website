# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Every file
[*]

# Set default charset
charset = utf-8

# Unix-style newlines with a clean newline ending
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

# 2 space indentation
indent_style = space
indent_size = 2
